@using WebPlatform.Models.Enum
@model List<Connection>
@*
   
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<p>@Model.Count()</p>
    @foreach (var conn in Model)
{
    <div id="@{conn.AppType.ToString();}" style="width:300px; height:300px; border: 2px solid red">
        <p>@conn.AppType</p>
        @Html.Partial("_InnerModule", conn)
    </div>
    
  
} 

